using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace StrengthTracker2.Core.Repository.Entities.WorkoutManagement
{
    public class UserWorkoutStatus
    {
        /// <summary>
        /// Primary key
        /// </summary>
        public int ID { get; set; }

        /// <summary>
        /// Associated Program
        /// </summary>
        public int ProgramID { get; set; }

        /// <summary>
        /// Associated Track
        /// </summary>
        public int TrackID { get; set; }

        /// <summary>
        /// Associated User
        /// </summary>
        public int UserID { get; set; }

        /// <summary>
        /// Step completed
        /// </summary>
        public int CompletedStep { get; set; }

        /// <summary>
        /// Group completed
        /// </summary>
        public int CompletedGroup { get; set; }

        /// <summary>
        /// Number of session completed
        /// </summary>
        public int SessionCompleted { get; set; }

        /// <summary>
        /// Counter to keep track of session/group/step
        /// </summary>
        public int StepGroupSessionCounter { get; set; }
        
        /// <summary>
        /// Session Status
        /// </summary>
        public int Status { get; set; }
    }
}
