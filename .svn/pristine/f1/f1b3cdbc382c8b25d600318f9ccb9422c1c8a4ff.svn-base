using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace StrengthTracker2.Core.Functional.DBEntities.WorkoutManagement
{
    public class SAQDetail
    {
        /// <summary>
        /// primary key
        /// </summary>
        public int Id { get; set; }

        /// <summary>
        /// Movement Type
        /// </summary>
        public int MovementTypeID { get; set; }

        /// <summary>
        /// Exercise
        /// </summary>
        public int ExerciseID { get; set; }

        /// <summary>
        /// UOM
        /// </summary>
        public int UOMID { get; set; }

        /// <summary>
        /// Target score
        /// </summary>
        public decimal TargetZScore { get; set; }

        /// <summary>
        /// Actual Score
        /// </summary>
        public decimal ActualScore { get; set; }

        /// <summary>
        /// Normative data
        /// </summary>
        public decimal NormativeData { get; set; }

        /// <summary>
        /// Associated UserID
        /// </summary>
        public int UserID { get; set; }


        /// <summary>
        /// Calculated SAQ Quotient
        /// </summary>
        public decimal SAQQuotient { get; set; }

        /// <summary>
        /// Evaluation Session number associated with the SAQStivity data
        /// </summary>
        public int SessionNumber { get; set; }

        /// <summary>
        /// SAQS Session Master ID
        /// </summary>
        public int SAQStivitySessionMasterID { get; set; }
